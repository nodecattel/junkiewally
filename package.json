{
  "name": "junkiewally",
  "version": "1.0.0",
  "description": "First Web3 wallet for Junkcoin blockchain, enabling JKC management and junkinals interaction",
  "private": false,
  "author": {
    "name": "NodeCattel",
    "url": "https://github.com/nodecattel"
  },
  "license": "MIT",
  "homepage": "https://github.com/nodecattel/junkiewally",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodecattel/junkiewally.git"
  },
  "bugs": {
    "url": "https://github.com/nodecattel/junkiewally/issues"
  },
  "keywords": [
    "junkcoin",
    "junkinals",
    "jkc",
    "web3",
    "cryptocurrency",
    "wallet",
    "browser-extension",
    "chrome-extension",
    "firefox-addon",
    "blockchain"
  ],
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/wif-test/**/*.js",
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ]
  },
  "scripts": {
    "dev": "bun build.ts --watch",
    "fire": "bun build.ts --watch --firefox",
    "chrome": "bun build.ts",
    "firefox": "bun build.ts --firefox",
    "both": "rm -rf dist && bun build.ts --firefox && bun build.ts",
    "release": "rm -rf dist && bun build.ts --firefox -c && bun build.ts -c",
    "sourcemap": "bun build.ts --sourcemap",
    "test": "bun build.ts --watch --no-interaction",
    "address-test": "jest ./wif-test/address-test.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@headlessui/react": "2.1.2",
    "@heroicons/react": "2.1.5",
    "@noble/hashes": "1.4.0",
    "axios": "^1.7.9",
    "bel-ord-utils": "^0.2.11",
    "bells-inscriber": "^0.2.8",
    "bs58check": "^4.0.0",
    "classnames": "2.5.1",
    "dayjs": "^1.11.13",
    "eth-rpc-errors": "4.0.3",
    "events": "^3.3.0",
    "i18next": "^24.2.0",
    "i18next-icu": "^2.3.0",
    "immer": "^10.1.1",
    "intl-messageformat": "^10.7.10",
    "junkcoinhdw": "^0.1.1",
    "junkcoinjs-lib": "^0.0.7",
    "junkcoinpair": "^0.1.1",
    "koa-compose": "4.1.0",
    "nintondo-bip39": "^1.0.0",
    "nintondo-browser-passworder": "^0.0.1",
    "nintondo-sdk": "^0.1.15",
    "qr-code-styling": "1.6.0-rc.1",
    "rc-progress": "4.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.52.2",
    "react-hot-toast": "2.4.1",
    "react-i18next": "^15.4.0",
    "react-intersection-observer": "9.13.0",
    "react-loading-icons": "1.1.0",
    "react-router-dom": "6.26.0",
    "reflect-metadata": "0.2.2",
    "tiny-secp256k1": "^2.2.3",
    "webextension-polyfill": "^0.12.0",
    "zustand": "4.5.4"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.14.0",
    "@types/chrome": "^0.0.270",
    "@types/koa-compose": "^3.2.8",
    "@types/node": "^22.1.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/webextension-polyfill": "^0.12.1",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.20",
    "bun-types": "^1.1.21",
    "esbuild": "^0.24.0",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-svg": "^0.1.0",
    "esbuild-plugin-svgj": "^1.0.3",
    "esbuild-plugin-wasm": "^1.1.0",
    "esbuild-plugins-node-modules-polyfill": "^1.6.3",
    "esbuild-sass-plugin": "^3.3.1",
    "esbuild-svg": "^1.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "postcss-modules": "^6.0.0",
    "sass": "^1.69.7",
    "sass-loader": "^16.0.0",
    "svgo": "^3.2.0",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  }
}
